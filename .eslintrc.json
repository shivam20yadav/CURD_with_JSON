{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": "off",
        "camelcase": "off",
        "no-unused-vars": "off",
        "import/no-extraneous-dependencies": "off",
        "import/no-unresolved": "off",
        "no-cond-assign": "off",
        "no-continue": "off",
        "no-plusplus": "off",
        "no-parm-reassign": "off",
        "no-bitwise": "off",
        "array-callback-return": "off",
        "no-param-reassign": "off"
    }
}
